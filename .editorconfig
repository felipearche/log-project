root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_style = space
indent_size = 4

[data/mini_tokens.json]
insert_final_newline = false

[data/synth_labels.json]
insert_final_newline = false

[data/synth_tokens.json]
insert_final_newline = false
